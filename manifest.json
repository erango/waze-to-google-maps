{
  "name": "Waze to Google Maps",
  "description": "Open waze:// links in Google Maps",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "permissions": ["activeTab"],
      "js": ["main.js"]
    }
  ],
  "page_action": {
    "default_icon": {
      "24": "map.png",
      "36": "map36.png",
      "48": "map48.png",
      "128": "map128.png"
    }
  },
  "icons": {
    "24": "map.png",
    "36": "map36.png",
    "48": "map48.png",
    "128": "map128.png"
  }
}
